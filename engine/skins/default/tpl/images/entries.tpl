<tr>
	<td><input type="checkbox" name="files[]" value="{id}"></td>
	<td>{id}</td>
	<td class="insert-file"><span class="pull-left">{insert_file}</span></td>
	<td class="insert-thumb"><span class="text-center">{insert_thumb}</span></td>
	<td class="insert-preview"><span class="pull-right">{insert_preview}</span></td>
	[preview]<td>{preview_img} {preview_size}</td>[/preview]
	<td><a href="{edit_link}" title="{l_edit}"><span class="img-title">{file_name}</span></a></td>
	<td class="img-src">{view_file}</td>
	<td>{view_thumb}</td>
	<td><span class="img-width">{width}</span>x<span class="img-height">{height}</span></td>
	<td nowrap="nowrap"><span class="img-size">{size}</span></td>
	<td>{folder}</td>
	<td>{user}</td>
</tr>